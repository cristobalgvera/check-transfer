{
  "version": 1,
  "projects": {
    "add-recipient": {
      "projectType": "library",
      "root": "libs/client/add-recipient",
      "sourceRoot": "libs/client/add-recipient/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/add-recipient"],
          "options": {
            "jestConfig": "libs/client/add-recipient/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/client/add-recipient/**/*.ts",
              "libs/client/add-recipient/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "models": {
      "root": "libs/shared/models",
      "sourceRoot": "libs/shared/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/models/**/*.ts"]
          }
        }
      },
      "tags": []
    },
    "material": {
      "projectType": "library",
      "root": "libs/client/material",
      "sourceRoot": "libs/client/material/src",
      "prefix": "material",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/material"],
          "options": {
            "jestConfig": "libs/client/material/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/client/material/**/*.ts",
              "libs/client/material/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/client/shared-ui",
      "sourceRoot": "libs/client/shared-ui/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/shared-ui"],
          "options": {
            "jestConfig": "libs/client/shared-ui/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/client/shared-ui/**/*.ts",
              "libs/client/shared-ui/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "transfer-app": {
      "projectType": "application",
      "root": "apps/transfer-app",
      "sourceRoot": "apps/transfer-app/src",
      "prefix": "transfer-app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/transfer-app",
            "index": "apps/transfer-app/src/index.html",
            "main": "apps/transfer-app/src/main.ts",
            "polyfills": "apps/transfer-app/src/polyfills.ts",
            "tsConfig": "apps/transfer-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/transfer-app/src/favicon.ico",
              "apps/transfer-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/transfer-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/transfer-app/src/environments/environment.ts",
                  "with": "apps/transfer-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "transfer-app:build:production"
            },
            "development": {
              "browserTarget": "transfer-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/transfer-app/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "transfer-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/transfer-app/src/**/*.ts",
              "apps/transfer-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/transfer-app"],
          "options": {
            "jestConfig": "apps/transfer-app/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "transfer-app-e2e": {
      "root": "apps/transfer-app-e2e",
      "sourceRoot": "apps/transfer-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/transfer-app-e2e/cypress.json",
            "devServerTarget": "transfer-app:serve:development",
            "tsConfig": "apps/transfer-app-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "transfer-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/transfer-app-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["transfer-app"]
    },
    "transfer-history": {
      "projectType": "library",
      "root": "libs/client/transfer-history",
      "sourceRoot": "libs/client/transfer-history/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/transfer-history"],
          "options": {
            "jestConfig": "libs/client/transfer-history/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/client/transfer-history/**/*.ts",
              "libs/client/transfer-history/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "transfer-money": {
      "projectType": "library",
      "root": "libs/client/transfer-money",
      "sourceRoot": "libs/client/transfer-money/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client/transfer-money"],
          "options": {
            "jestConfig": "libs/client/transfer-money/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/client/transfer-money/**/*.ts",
              "libs/client/transfer-money/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
